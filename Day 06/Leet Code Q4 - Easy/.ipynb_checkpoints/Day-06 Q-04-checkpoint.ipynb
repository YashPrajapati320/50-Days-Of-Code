   "source": [
    "def strStr(haystack,needle):\n",
    "    if needle == \"\":\n",
    "        return 0\n",
    "    elif needle in haystack:\n",
    "        return haystack.index(needle)\n",
    "    else:\n",
    "        return -1\n",
    "    \n",
    "strStr('aaa','a')"
   ]
  },
