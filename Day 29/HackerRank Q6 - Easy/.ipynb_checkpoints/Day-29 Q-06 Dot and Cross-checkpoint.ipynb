   "source": [
    "import numpy\n",
    "n=int(input())\n",
    "arr1,arr2=[(numpy.array([list(map(int,input().split())) for _ in range(n)])) for _ in range(2)]\n",
    "print (numpy.dot(arr1,arr2))"
   ]
