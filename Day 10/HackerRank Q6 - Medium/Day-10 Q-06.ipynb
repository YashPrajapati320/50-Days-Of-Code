   "source": [
    "# Alphabet Rangoli"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "Sample Input\n",
    "\n",
    "5\n",
    "\n",
    "Sample Output\n",
    "\n",
    "--------e--------\n",
    "------e-d-e------\n",
    "----e-d-c-d-e----\n",
    "--e-d-c-b-c-d-e--\n",
    "e-d-c-b-a-b-c-d-e\n",
    "--e-d-c-b-c-d-e--\n",
    "----e-d-c-d-e----\n",
    "------e-d-e------\n",
    "--------e--------"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5\n",
      "--------e--------\n",
      "------e-d-e------\n",
      "----e-d-c-d-e----\n",
      "--e-d-c-b-c-d-e--\n",
      "e-d-c-b-a-b-c-d-e\n",
      "--e-d-c-b-c-d-e--\n",
      "----e-d-c-d-e----\n",
      "------e-d-e------\n",
      "--------e--------\n"
     ]
    }
   ],
   "source": [
    "import string\n",
    "N = int(input())\n",
    "alphabet = string.ascii_lowercase\n",
    "\n",
    "for i in range(N-1, -N, -1):\n",
    "    row = [\"-\"]*(4*N-3)\n",
    "    for j in range(0, N - abs(i)):\n",
    "        row[2*(N-1+j)] = alphabet[abs(i)+j]\n",
    "        row[2*(N-1-j)] = alphabet[abs(i)+j]\n",
    "    print(\"\".join(row))"
   ]
  },
